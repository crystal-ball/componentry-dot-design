{
  "name": "componentry-dot-design",
  "version": "0.0.0",
  "description": "Componentry documentation application",
  "keywords": [
    "componentry",
    "documentation"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/componentry-dot-design.git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run test:lint",
    "test:lint": "NODE_ENV=test eslint --format=pretty --ext .js,.ts cypress src",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress --abort-on-container-exit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write '*.{js,json,md,json}' '**/*.{md,js,json,scss}'"
  },
  "dependencies": {
    "@babel/runtime": "7.8.7",
    "@emotion/core": "10.0.28",
    "@hot-loader/react-dom": "16.13.0",
    "@mdx-js/react": "1.5.7",
    "classnames": "2.2.6",
    "componentry": "3.0.0-rc.7",
    "emotion-theming": "10.0.27",
    "prism-react-renderer": "1.0.2",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-hot-loader": "4.12.20",
    "react-router-dom": "5.1.2",
    "svg-symbol-sprite-loader": "4.0.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "1.4.0",
    "@crystal-ball/commitizen-base": "2.4.0",
    "@crystal-ball/prettier-base": "1.6.0",
    "@crystal-ball/webpack-base": "9.1.0",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.1",
    "@types/jest": "25.1.4",
    "babel-plugin-emotion": "10.0.29",
    "eslint-config-eloquence": "13.6.0",
    "husky": "4.2.3",
    "jest": "25.2.3",
    "jest-emotion": "10.0.32"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  }
}
