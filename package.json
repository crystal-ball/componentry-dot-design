{
  "name": "componentry.design",
  "version": "0.0.0",
  "description": "Componentry documentation application",
  "keywords": [
    "componentry",
    "documentation"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/componentry.design"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "commit": "cz commit",
    "start": "NODE_ENV=development webpack serve --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run test:lint",
    "test:lint": "NODE_ENV=test eslint --format=pretty . --ignore-pattern 'public'",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress --abort-on-container-exit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write . !CHANGELOG.md !coverage !public"
  },
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "@hot-loader/react-dom": "17.0.1",
    "@mdx-js/react": "1.6.22",
    "classnames": "2.2.6",
    "componentry": "3.0.0",
    "prettier": "2.2.1",
    "pretty-format": "26.6.2",
    "prism-react-renderer": "1.1.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.2.0",
    "svg-symbol-sprite-loader": "5.0.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "3.0.0",
    "@crystal-ball/commitizen-base": "2.10.0",
    "@crystal-ball/prettier-base": "2.6.0",
    "@crystal-ball/webpack-base": "10.2.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@types/jest": "26.0.20",
    "eslint-config-eloquence": "17.3.0",
    "eslint-plugin-mdx": "1.8.2",
    "jest": "26.6.3",
    "node-sass-utils": "1.1.3",
    "sass": "1.32.7",
    "typescript": "4.1.4"
  }
}
